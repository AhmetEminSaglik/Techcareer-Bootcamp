@model dynamic

@{
    ViewBag.Title = "Index";
    var book = Model.Item1;
    var student = Model.Item2;
}

<h1>Book View</h1>
<br />
<h2>Book Data </h2>

@*<p>Id: @book.Id</p>*@
<p>Name: @book.Name</p>
<p>Description: @book.Desc</p>
<p>Number Of Page: @book.pageNumber</p>

<hr />

<h2>Student Data</h2>

@*<p>Id: @student.Id</p>*@
<p>Name: @student.Name</p>
<p>Lastname: @student.LastName</p>
<p>Email: @student.email</p>
<p>TC No: @student.TcNo</p>
<p>Major: @student.Major</p>
<p>Registered Year: @student.RegisteredYear</p>

